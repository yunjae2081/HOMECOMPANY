<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="videolistvo.dao.VideoListDAO">	
	<select id="selectNo" resultType="videolistvo">
		<![CDATA[ 
		select no as vNo 
		  from tbl_test3
		 limit 0, 16;		
		]]> 
	</select>
	
	<select id="selectExtend" resultType="videolistvo">
		<![CDATA[ 
		select no as vNo 
		  from tbl_test3
		 limit #{no}, 8;		
		]]> 
	</select>

</mapper>